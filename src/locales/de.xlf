        <source>Locations</source>
        <target state="translated">Standorte</target>
        <context-group purpose="location">
          <context context-type="sourcefile">src/app/pages/peering/peering-locations/peering-locations.component.html</context>
          <context context-type="linenumber">1</context>
        </context-group>
